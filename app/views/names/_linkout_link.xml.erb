  <Link>
    <LinkId><%= name.id %></LinkId>
    <ProviderId><%= @provider_id %></ProviderId>
    <ObjectSelector>
      <Database>Taxonomy</Database>
      <ObjectList>
        <% if name.ncbi_taxonomy %>
          <ObjId><%= name.ncbi_taxonomy %></ObjId>
        <% else %>
          <Query><%= name.base_name %>[Name]</Query>
        <% end %>
      </ObjectList>
    </ObjectSelector>
    <ObjectUrl>
      <Base>&base.url;</Base>
      <Rule><%= name.id %></Rule>
      <UrlName><%= name.abbr_name_raw %> | SeqCode Registry</UrlName>
      <SubjectType>taxonomy/phylogenetic</SubjectType>
    </ObjectUrl>
  </Link>
