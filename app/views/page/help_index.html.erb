<% provide(:title, 'Help Central') %>
<h1>Frequently Asked Questions</h1>
<hr/>
<ul>
  <% old_cat = nil %>
  <% @topics.each do |cat, topics| %>
    <% if cat != old_cat %>
        <div class="mt-3 text-muted"><b>[ <%= cat %> ]</b></div>
    <% end %>

    <% topics.each do |k, topic| %>
      <li><%= link_to(topic, help_path(k)) %></li>
    <% end %>
  <% end %>
</ul>
<br/>
