<%= adaptable_entry(list, strain.title, strain) do |entry| %>
  <%= adaptable_value(entry, 'Name(s)') do %>
    <% strain.typified_names.each do |name| %>
      <%= display_link(name) %><br/>
    <% end %>
  <% end %>
  <%= adaptable_value(entry, 'Strain numbers') do %>
    <%= strain_html(strain, no_strain_info: true, no_title: true) %>
  <% end %>
  <%= adaptable_value(entry, 'Lookups') do %>
    <%= strain_info_button(strain, nil, 'StrainInfo') %>
  <% end %>
<% end %>
